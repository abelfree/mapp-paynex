# Monetag SDK integration
# Paste exact values from your Monetag dashboard/docs.
MONETAG_SDK_SRC=
MONETAG_MAIN_ZONE=
# Optional. If empty, server uses show_<MONETAG_MAIN_ZONE>.
MONETAG_SHOW_FN=

# Optional security token for postback endpoint.
# If set, Monetag must call /api/monetag/postback?token=<this_value>
MONETAG_POSTBACK_TOKEN=

# Keep true for local testing, set false in production.
ALLOW_SIMULATE_VALUED=true
